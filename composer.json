{
  "name": "ajant/db-mock-library",
  "authors": [
    {
      "name": "Jovanovic Aleksandar",
      "email": "ajant.github@gmail.com"
    }
  ],
  "license": "MIT",
  "description": "Db mocking & dummy data management library",
  "homepage": "https://github.com/ajant/DbMockLibrary",
  "require": {
    "php": ">=7.1",
    "ajant/simple-array-library": ">=2",
    "mongodb/mongodb": "~1.7.0"
  },
  "require-dev": {
    "php": ">=7.4",
    "phpunit/phpunit": ">=9",
    "mockery/mockery": "^1.4",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "DbMockLibrary\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DbMockLibrary\\Test\\": "test"
    }
  },
  "scripts": {
    "test-u": "vendor/bin/phpunit -c test/phpunit.xml test",
    "test-coverage-u": "vendor/bin/phpunit --coverage-clover build/logs/clover.xml -v -c test/phpunit.xml test"
  }
}
